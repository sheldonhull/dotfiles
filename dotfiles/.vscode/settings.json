            "zeroWidth": true
        },
        "202d": {
            "description": "left-to-right override",
            "level": "error",
            "zeroWidth": true
        },
        "202e": {
            "description": "right-to-left override",
            "level": "error",
            "zeroWidth": true
        },
        "fffc": {
